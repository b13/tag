includes:
    - vendor/phpstan/phpstan/conf/bleedingEdge.neon
    - vendor/saschaegerer/phpstan-typo3/extension.neon

parameters:
  level: 5

  paths:
    - %currentWorkingDirectory%/

  excludePaths:
    analyseAndScan:
      - */node_modules/*
      - vendor/*
      - public/*
      - ./.php-cs-fixer.php
